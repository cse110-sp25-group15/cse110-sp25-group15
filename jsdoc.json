{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./src/components/", "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true,
        "tutorials": "./Tutorials",
        "template": "node_modules/docdash"
    }
}